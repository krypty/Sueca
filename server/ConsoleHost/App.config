<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.serviceModel>
    <services>
      <service behaviorConfiguration="SuecaServices.SuecaBehavior"
        name="SuecaServices.ServiceSueca">
        <!--<endpoint address="Sueca"
                  binding="netTcpBinding"
                  contract="SuecaContracts.ISuecaContract">
        </endpoint>-->
        <endpoint address="" binding="wsDualHttpBinding" contract="SuecaContracts.ISuecaContract" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8731/SuecaServices/" />
            <!--<add baseAddress="net.tcp://localhost:8732/SuecaServices/" />-->
          </baseAddresses>
        </host>
        <endpoint address="mex"
 binding="mexHttpBinding"
 contract="IMetadataExchange" />
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="SuecaServices.SuecaBehavior">
          <serviceMetadata httpGetEnabled="true" />
          <!-- mettre à true pour la phase de tests seulement -->
          <serviceDebug includeExceptionDetailInFaults="true" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>